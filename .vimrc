set nu
set hlsearch
set incsearch
set backspace=2
set autoindent
set showmode
set bg=dark
set ruler
set shiftwidth=4
set softtabstop=4
set tabstop=4
set foldenable
set foldmethod=manual
set completeopt=preview,menu
set nobackup
set noswapfile
set autowrite
set showmatch
set smartindent
set mouse=a
set clipboard=unnamedplus
inoremap ( ()<ESC>i
inoremap [ []<ESC>i
inoremap { {}<ESC>i
inoremap ' ''<ESC>i
inoremap " ""<ESC>i
syntax on
